{
  "name": "forum",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=16.14.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-static": "next build &&  next export",
    "start": "next start",
    "lint": "next lint",
    "commit": "cz",
    "clean": "rimraf node_modules &&  yarn cache clean",
    "windows-clean": "del node_modules",
    "prettier-format": "prettier --write  **/*.{scss,css} **/*.{jsx,tsx,js,ts}",
    "eslint-fix": "eslint --ext .tsx --ext .ts  pages/*/** --fix",
    "prepare-husky": "yarn husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@types/qs": "^6.9.7",
    "antd": "^4.23.2",
    "axios": "1.7.7",
    "dayjs": "^1.11.13",
    "next": "14.2.15",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.79.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "22.7.5",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.5",
    "cz-customizable": "^7.0.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.6.3"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "yarn prettier-format",
      "git add ."
    ],
    "*.ts?(x)": [
      "yarn prettier-format",
      "eslint --fix",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
