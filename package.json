{
  "name": "forum",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=16.14.1"
  },
  "scripts": {
    "dev": "next dev",
    "proxy-dev": "node script/cross-domain-proxy.js",
    "build": "next build",
    "build-static": "next build &&  next export",
    "start": "next start",
    "lint": "next lint",
    "commit": "cz",
    "clean": "rimraf node_modules &&  yarn cache clean",
    "windows-clean": "del node_modules",
    "prettier-format": "prettier --write  **/*.{scss,css} **/*.{jsx,tsx,js,ts}",
    "eslint-fix": "eslint --ext .tsx --ext .ts  pages/*/** --fix",
    "prepare-husky": "yarn husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "antd": "^4.23.2",
    "axios": "^0.27.2",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.54.9",
    "@types/qs": "^6.9.7"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.5",
    "cz-customizable": "^7.0.0",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "4.8.3"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.18"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "yarn prettier-format",
      "git add ."
    ],
    "*.ts?(x)": [
      "yarn prettier-format",
      "eslint --fix",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
