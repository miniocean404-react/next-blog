{
  //其他配置
  "i18n-ally.localesPaths": ["./locales"], // 翻译文件夹路径 (相对于项目根目录)，你也可以使用 Glob 匹配模式。
  "i18n-ally.enabledFrameworks": ["custom"],
  "i18n-ally.keystyle": "nested", // 翻译路径格式
  "i18n-ally.enabledParsers": ["ts", "json"], // 指定启用文件格式
  "i18n-ally.namespace": true,
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.sourceLanguage": "zh",
  "npm.exclude": ["**/.next/**"],

  "cSpell.words": ["Element Plus", "element-plus"], // code spell checker总是认为是对的单词列表
  "vite.devCommand": "yarn run dev -- --", // 运行命令

  "unocss.root": "./",
  "iconify.includes": ["ep"],

  // 规定 ESlint 插件需要校验的语言类型
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html", "vue", "markdown", "json", "jsonc"],
  // 老版本 ESlint 插件需要校验的语言类型
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html", "vue", "markdown"]
}
